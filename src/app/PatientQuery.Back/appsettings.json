{
  "Connections": {
    "Default": "Server=db;User Id=db_user;Password=db_password;Database=db_patient_query;",
    "Omop": "Server=db_omop;User Id=db_user;Password=db_password;Database=db_omop;",
    "Iris": "Server=iris;Port=1972;User ID=_SYSTEM;Password=SYS;Namespace=FHIRServer"
  },
  "AppVersion": "",
  "NLog": {
    "targets": {
      "console": {
        "type": "ColoredConsole",
        "layout": "${time}|${pad:padding=5:inner=${level:uppercase=true}}|${logger:shortName=true}|${message} ${exception:format=toString,Data}"
      },
      "file": {
        "type": "File",
        "fileName": "${basedir}/logs/${environment:ASPNETCORE_ENVIRONMENT}_${shortdate}.txt",
        "layout": "${longdate}|${pad:padding=5:inner=${level:uppercase=true}}|${logger}|${message} ${exception:format=toString,Data}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "maxLevel": "Debug",
        "final": true
      },
      {
        "logger": "*",
        "writeTo": "console"
      },
      {
        "logger": "*",
        "writeTo": "file"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OpenAi": {
    "Url": "https://api.openai.com",
    "ApiKey": "your-openai-api-key-here",
    "TransformRequest": {
      "Model": "gpt-4-1106-preview",
      "Temperature": 0,
      "TopP": 1,
      "MaxTokens": 768,
      "PresencePenalty": 0,
      "FrequencyPenalty": 0
    },
    "MedicationInfoRequest": {
      "Model": "gpt-4",
      "Temperature": 0,
      "TopP": 1,
      "MaxTokens": 56,
      "PresencePenalty": 0,
      "FrequencyPenalty": 0
    }
  }
}